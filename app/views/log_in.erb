<form class="form-position" action="/log_in" method="POST">
  <div class="col-xs-12 col-md-5 mb-3">
    <label for="user_email" class="form-label">User email:</label>
    <input name="user[email]" type="text" class="form-control" autocomplete="on">
  </div>

  <div class="col-xs-12 col-md-5 mb-3">
    <label for="exampleInputPassword1" class="form-label">Password:</label>
    <input name="user[password]" type="password" class="form-control" id="exampleInputPassword1">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
